.swiper-tab{
  height: 30px;
  line-height: 30px;
  overflow: hidden;
}
.swiper-tab-one{
  height: 29px;
  line-height: 30px;
  width: 33.3%;
  text-align: center;
  float: left;
}
.swiper-tab-on{
  border-bottom: 1px solid #3A5FCD;
  color: #3A5FCD;
}
.themes-list{
  display: block;

}
.themes-list-one{
  display: block;
  position: relative;
  padding: 5px 10px 5px 10px;
  overflow: hidden;
  border-bottom: 1px solid #ececec;
  height: 60px;
  font-size: 12px;
  color: #999999;
}
.list-one-img{
  position: absolute;
  height: 60px;
  width: 60px;

}
.list-one-img image{
  width: 60px;
  height: 60px;
  border-radius: 5px;
}
.list-one-content{
  margin-left: 80px;
  vertical-align: middle
}
.swiper-box{ display: block; height: 100%; width: 100%; overflow: hidden; }