.swiper-tab{
  height: 30px;
  line-height: 30px;
  overflow: hidden;
  position: fixed;
  z-index: 1;
  background-color: #ffffff;
  width: 100%;
}
.swiper-tab-one{
  height: 29px;
  line-height: 30px;
  width: 33.3%;
  text-align: center;
  float: left;
}
.swiper-tab-on{
  border-bottom: 1px solid #3A5FCD;
  color: #3A5FCD;
}

.swiper-box{ display: block; height: 100%; width: 100%; overflow: hidden; }
.themes-swiper{padding-bottom: 15px;}
.themes-list{
  display: block;
  height: 100%;
  z-index: 2;
  position: relative;
  margin-top: 30px;
}
.themes-list-one{
  display: block;
  position: relative;
  padding: 5px 10px 5px 10px;
  overflow: hidden;
  border-bottom: 1px solid #ececec;
  height: 40px;
  font-size: 12px;
  color: #999999;
}
.list-one-img{
  position: absolute;
  height: 40px;
  width: 40px;

}
.list-one-img image{
  width: 40px;
  height: 40px;
  border-radius: 5px;
}
.list-one-content{
  margin-left: 60px;
  vertical-align: middle
}
